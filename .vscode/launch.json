{
    "version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "acryps-events (local)",
			"preLaunchTask": "build",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/server/dist/main.js",
			"cwd": "${workspaceFolder}/server",
			"outFiles": [
				"${workspaceFolder}/**/*.js"
			],
			"env": {
				"PGHOST": "10.22.0.1",
				"PGUSER": "acryps_events",
				"PGPASSWORD": "acryps_events",
				"PGDATABASE": "acryps_events"
			}
		}
	]
}